package mypack;

import java.net.MulticastSocket;

public class Test {
	public static void main(String[] args) {
		int[] arr1= {6,2,4};
		int[] arr2= {3,1};
		
//		for(int i=0;i<arr1.length;i++) {
//			
//		}
		int re = 0;
		for(int i:arr1) {
			re*=10;
			re = re + i;	
		}
		int re2 = 0;
		for(int i:arr2) {
			re2*=10;
			re2 = re+i;
		}
		System.out.println(re);
		System.out.println(re2);
	}

}
